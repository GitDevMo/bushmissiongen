    <SimMission.Calculator id="CLTR_RunwayLandingCheck" InstanceId="{3A7218C5-4549-40A8-9A4E-749ACB60E6FE}">
      <SimBase.Descr>Landing check</SimBase.Descr>
      <WorldBase.Activated>true</WorldBase.Activated>
      <CalculatorFormula>
        [LandingComplete]
        if{	1 }	els{ 0 }
      </CalculatorFormula>
      <CalculatorParameterList>
        <FormulaParameter NameInFormula="OnGround">
          <CalculatorFormula>
            (A:SIM ON GROUND, Boolean) 0 &gt;
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="FirstTouchDown">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            [OnGround]
            [FirstTouchDown] !
            and
            [FirstTouchDownDone] !
            and
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="FirstTouchDownDone">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            [FirstTouchDown]
            [FirstTouchDownDone]
            max
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="OverRunwayArea">
          <CalculatorFormula>
            (A:ATC RUNWAY RELATIVE POSITION X, meters) abs
            0.6 (A:ATC RUNWAY WIDTH, meters) *
            &lt;

            (A:ATC RUNWAY RELATIVE POSITION Z, meters) abs
            0.6 (A:ATC RUNWAY LENGTH, meters) *
            &lt;
            and
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="GearDeployed">
          <CalculatorFormula>
            (A:IS GEAR RETRACTABLE, Boolean) 0 ==
            (A:GEAR TOTAL PCT EXTENDED, percentage) 1 ==
            or
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="WrongLanding">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            (A:PLANE HEADING DEGREES TRUE, degrees)
            (A:ATC RUNWAY HEADING DEGREES TRUE, degrees) -
			900 +
            360	%
			180 -
            abs
            90 &gt;
            [FirstTouchDown]
            and
            [OverRunwayArea]
            and
            [WrongLanding]
            or
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="Timer_AfterLanding">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            (A:AIRSPEED TRUE, Knots) 20 &lt;
            [Timer_InsideOfRunway] 15 &gt;
            or
            [OnGround]
            and
            [OverRunwayArea]
            and
            [WrongLanding] not
            and
            [GearDeployed]
            and
            if{
            [Timer_AfterLanding]
            [dtime]
            +
            }
            els{
            0
            }
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="LandingComplete">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            [Timer_AfterLanding] 1 &gt;
            [LandingComplete]
            or
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="Timer_WrongLanding">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            [WrongLanding] if{
            [Timer_WrongLanding]
            [dtime]
            +
            }
            els{
            0
            }
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="Timer_TouchdownCancelled">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            [LandingComplete] not
            [OnGround] not
            [FirstTouchDownDone]
            and
            and
            if{
            [Timer_TouchdownCancelled]
            [dtime]
            +
            }
            els{
            0
            }
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="Timer_OutOfRunway">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            [LandingComplete] not
            [OnGround]
            [OverRunwayArea] not
            and
            and
            if{
            [Timer_OutOfRunway]
            [dtime]
            +
            }
            els{
            0
            }
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="Timer_InsideOfRunway">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            [LandingComplete] not
            [OnGround]
            [OverRunwayArea]
            and
            and
            if{
            [Timer_InsideOfRunway]
            [dtime]
            +
            }
            els{
            0
            }
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="EndOfChallenge">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            [LandingComplete]
            (A:ATC RUNWAY DISTANCE, feet) 4000 &gt;
            and
            [OnGround] not
            and
            [EndOfChallenge]
            or
          </CalculatorFormula>
        </FormulaParameter>
        <FormulaParameter NameInFormula="Timer_EndOfChallenge">
          <StartValue>0.000</StartValue>
          <CalculatorFormula>
            [EndOfChallenge]
            if{
            [Timer_EndOfChallenge]
            [dtime]
            +
            }
            els{
            0
            }
          </CalculatorFormula>
        </FormulaParameter>
